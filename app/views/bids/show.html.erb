<div class="page-header">
  <%= link_to bids_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_bid_path(@bid), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show bid</h1>
</div>

<dl class="dl-horizontal">
  <dt>Bid price:</dt>
  <dd><%= @bid.bid_price %></dd>

  <dt>Auction:</dt>
  <dd><%= @bid.auction %></dd>

  <dt>Bid date:</dt>
  <dd><%= @bid.bid_date %></dd>

</dl>
